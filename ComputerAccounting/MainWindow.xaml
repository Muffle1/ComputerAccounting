<Window x:Class="ComputerAccounting.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComputerAccounting" 
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        xmlns:viewmodels="clr-namespace:ComputerAccounting.ViewModels"
        xmlns:page="clr-namespace:ComputerAccounting.Pages" 
        mc:Ignorable="d"
        Title="MainWindow"
        MinHeight="500"
        MinWidth="700"
        Icon="Images/WindowIcon.png"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized">

    <Window.Resources>
        <DataTemplate DataType="{x:Type viewmodels:AuthenticationManagerViewModel}">
           <page:AuthenticationManager/>
        </DataTemplate>
    </Window.Resources>
    
    <Grid>
        <ContentControl Content=""/>
    </Grid>
</Window>
